<!DOCTYPE html>
<html>
<head>
  <title>Myapp</title>
  <%= stylesheet_link_tag    'bower_components/bootstrap/dist/css/bootstrap.min.css', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'bower_components/metisMenu/dist/metisMenu.min.css', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'dist/css/timeline.css', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'dist/css/sb-admin-2.css', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'bower_components/morrisjs/morris.css', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    'bower_components/font-awesome/css/font-awesome.min.css', media: 'all', 'data-turbolinks-track' => true %>

  <%= javascript_include_tag 'jquery.min', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag '//cdn.ckeditor.com/4.5.10/standard/ckeditor.js', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'bower_components/bootstrap/dist/js/bootstrap.min', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'bower_components/metisMenu/dist/metisMenu.min', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'dist/js/sb-admin-2', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id="wrapper">
  <%= render file: "includes/nav" %>

  <div id="page-wrapper">
    <%= yield %>
  </div>
  <!-- /#page-wrapper -->

</div>
<!-- /#wrapper -->


</body>
</html>
